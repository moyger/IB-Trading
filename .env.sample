# Edgerunner Environment Configuration
# Copy this file to .env and fill in your credentials

# Environment
ENVIRONMENT=dev

# Interactive Brokers
IBKR_HOST=127.0.0.1
IBKR_PORT=7497
IBKR_CLIENT_ID=1
IBKR_ACCOUNT=
IBKR_PAPER_TRADING=true

# Bybit
BYBIT_API_KEY=
BYBIT_API_SECRET=
BYBIT_TESTNET=true

# MetaTrader 5
MT5_SERVER=
MT5_LOGIN=
MT5_PASSWORD=

# Database
DATABASE_URL=sqlite:///data/edgerunner.db

# Redis Cache
REDIS_URL=redis://localhost:6379/0

# Monitoring
SLACK_WEBHOOK_URL=
DISCORD_WEBHOOK_URL=

# API
API_SECRET_KEY=your-secret-key-here
JWT_SECRET_KEY=your-jwt-secret-here

# Logging
LOG_LEVEL=INFO